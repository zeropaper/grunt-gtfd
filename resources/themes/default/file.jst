<html>
<head>
  <title><%= pageTitle %></title>
  <link type="text/css" rel="stylesheet" href="<%= assets %>/styles/styles.css" />
</head>
<body>
  <header>
    <h1><%= pageTitle %></h1>
  </header>

  <%= navigation %>
  <%= breadcrumb %>

  <div class="container content">
  <% blocks.forEach(function (block) { %>
    <div class="row">
      <div class="col-md-6"><%= block.documentation %></div>
      <div class="col-md-6"><%= block.code %></div>
    </div>
  <% }); %>
  </div>

  <footer>

  </footer>

  <script src="<%= assets %>/scripts/scripts.js"></script>
  <script>
  var config = <%= config %>;
  config.files = (config.files || []).map(function (filepath) {
    return { filepath: filepath };
  });
  config.el = document.body;
  new whatsupdoc(config);
  </script>
</body>
</html>
